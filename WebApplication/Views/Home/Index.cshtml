@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>CFO&nbsp;</h1>
    <p class="lead">Workout Of the Day from crossfitorlando.com</p>
</div>

<div ng-controller="DatepickerDemoCtrl" class="row">
    <div class="col-md-6">
        <div style="min-width:324px; min-height:290px;">
            <datepicker ng-model="dt" ng-change='changedate()' show-weeks="false" min-date="minDate" max-date="maxDate" class="well well-sm" custom-class="getDayClass(date, mode)"></datepicker>
        </div>
    </div>
    <div class="col-md-6">
        <div style="min-height:267px;" class="panel panel-primary">
            {{wod}} 
        </div>
    </div>
</div>